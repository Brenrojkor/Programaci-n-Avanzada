@model ProyectoG2_Pokedex.Models.EquipoLuchaModel

<div class="equipo-container">
    <h1 class="title">Eliminar Equipo</h1>
    <p>¿Estás seguro de que deseas eliminar el siguiente equipo?</p>
    <div>
        <dl class="row">
            <dt>Nombre Completo</dt>
            <dd>@Model.NombreEquipo</dd>
            <dt>IdUsuario</dt>
            <dd>@Model.Usuario</dd>
            
        </dl>
    </div>
    <form asp-action="Eliminar" method="post">
        <input type="hidden" asp-for="IdUsuario" />
        <button type="submit" class="btn btn-delete">Eliminar</button>
        <a asp-action="Usuarios" class="btn btn-secondary">Cancelar</a>
    </form>
</div>

